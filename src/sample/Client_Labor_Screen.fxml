<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="644.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Client_Labor_Controller">
   <children>
      <TableView fx:id="tbllview_db_labor" layoutX="13.0" layoutY="100.0" prefHeight="244.0" prefWidth="239.0">
        <columns>
          <TableColumn fx:id="col_db_name" prefWidth="143.0" text="Name" />
          <TableColumn fx:id="col_db_price" prefWidth="95.0" text="Price/Hour" />
        </columns>
      </TableView>
      <TableView fx:id="tblview_client_labor" layoutX="344.0" layoutY="94.0" prefHeight="244.0" prefWidth="273.0">
        <columns>
          <TableColumn fx:id="col_client_name" prefWidth="119.0" text="Name" />
          <TableColumn fx:id="col_client_price" prefWidth="92.0" text="Price/Hour" />
            <TableColumn fx:id="col_labor_quantity" prefWidth="61.0" text="Quantity" />
        </columns>
      </TableView>
      <Button fx:id="btn_back" layoutX="14.0" layoutY="29.0" mnemonicParsing="false" onMouseClicked="#go_back" text="Back" />
      <Button fx:id="btn_add_client_labor" layoutX="76.0" layoutY="354.0" mnemonicParsing="false" onMouseClicked="#add_client_labor" text="Add to Client's Labor" />
      <Button fx:id="btn_remove_client_labor" layoutX="399.0" layoutY="354.0" mnemonicParsing="false" onMouseClicked="#remove_client_labor" text="Remove from Client's Labor" />
      <Label layoutX="89.0" layoutY="60.0" text="Labor Database">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="417.0" layoutY="60.0" text="Client's Labor">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="277.0" layoutY="183.0" text="Quantity" />
      <TextField fx:id="txtfield_quantity" alignment="CENTER" layoutX="277.0" layoutY="204.0" prefHeight="25.0" prefWidth="46.0" text="1.0" />
   </children>
</AnchorPane>
