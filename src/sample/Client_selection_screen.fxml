<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>


<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/15.0.1" fx:controller="sample.Client_selection_controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <FlowPane alignment="TOP_CENTER" hgap="2.0" orientation="VERTICAL" prefHeight="400.0" prefWidth="400.0">
         <children>
            <Label text="Select sample.Harmonic_Client Account:" textAlignment="CENTER">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button fx:id="button_new_client" mnemonicParsing="false" onMouseClicked="#create_new_client" text="New_client" translateY="15.0" />
            <Button fx:id="button_client_one" mnemonicParsing="false" onMouseClicked="#get_client_one" text="Client_one" translateY="30.0" />
            <Button fx:id="button_client_two" mnemonicParsing="false" onMouseClicked="#get_client_two" text="Client_two" translateY="45.0" />
            <Button fx:id="button_client_three" mnemonicParsing="false" onMouseClicked="#get_client_three" text="Client_three" translateY="60.0" />
            <Button fx:id="button_client_four" mnemonicParsing="false" onMouseClicked="#get_client_four" text="Client_four" translateY="75.0" />
            <Button fx:id="button_client_five" mnemonicParsing="false" onMouseClicked="#get_client_five" text="Client_five" translateY="90.0" />
         </children>
      </FlowPane>
   </children>
</GridPane>
